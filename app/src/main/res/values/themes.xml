        <item name="colorPrimary">@color/blue_grey</item>
        <item name="colorPrimaryVariant">@color/blue_grey_variant</item>
        <item name="colorOnPrimary">@color/blue_grey_on</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/yellow_orange</item>
